{
  "name": "bike-walk-life",
  "version": "0.1.0",
  "private": true,
  "description": "The source code for the Bike Walk Life blog.",
  "keywords": [
    "forestry"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnridesabike/bikewalklife.git"
  },
  "license": "GPL-3.0-or-later",
  "author": "John Jackson",
  "scripts": {
    "analyze": "npm-run-all build analyze:*",
    "analyze:js": "source-map-explorer _site/**/*.js _site/**/*.js.map",
    "build": "npm-run-all -l build:rescript build:assets build:html",
    "build:assets": "NODE_ENV=production webpack",
    "build:html": "ELEVENTY_EXPERIMENTAL=true ELEVENTY_ENV=production eleventy --quiet",
    "build:rescript": "bsb -make-world",
    "clean": "npm-run-all -l --parallel clean:*",
    "clean:rescript": "bsb -clean-world -make-world",
    "clean:site": "rm -r _site",
    "postinstall": "bsb -make-world",
    "prestart": "NODE_ENV=development webpack",
    "serve": "serve _site",
    "start": "npm-run-all -l --parallel start:*",
    "start:assets": "NODE_ENV=development webpack --watch",
    "start:html": "ELEVENTY_EXPERIMENTAL=true ELEVENTY_ENV=development eleventy --serve --port=8080",
    "start:rescript": "bsb -w"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-img": "^0.7.8",
    "@babel/core": "^7.12.10",
    "@fontsource/cooper-hewitt": "^4.1.0",
    "@fontsource/source-serif-pro": "^4.1.0",
    "@reach/visually-hidden": "^0.12.0",
    "@ryyppy/rescript-promise": "^0.0.2",
    "acutis-lang": "^0.7.0",
    "babel-loader": "^8.2.2",
    "browserslist": "^4.16.1",
    "bs-fetch": "^0.6.2",
    "bs-platform": "^8.4.2",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "fast-glob": "^3.2.5",
    "feather-icons": "^4.28.0",
    "html-minifier": "^4.0.0",
    "js-yaml": "^4.0.0",
    "markdown-it": "^12.0.4",
    "mini-css-extract-plugin": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.4",
    "postcss-custom-media": "^8.0.0",
    "postcss-loader": "^4.2.0",
    "postcss-normalize": "^9.0.0",
    "postcss-preset-env": "^6.7.0",
    "re-formality": "^4.0.0-beta.10",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "reason-react": "^0.9.1",
    "serve": "^11.3.2",
    "source-map-explorer": "^2.5.2",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.17.0",
    "webpack-cli": "^4.4.0",
    "webpack-manifest-plugin": "^3.0.0"
  },
  "devDependencies": {}
}
