{
  "name": "bike-walk-life",
  "private": true,
  "description": "A simple starter blog to get up and developing quickly with Forestry & Gatsby",
  "license": "MIT",
  "version": "0.1.0",
  "author": "kendall strautman",
  "scripts": {
    "build": "npm run re:cleanbuild && gatsby build",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "forestry-preview": "gatsby develop -p 8080 -H 0.0.0.0",
    "gatsby:clean": "gatsby clean",
    "re:clean": "bsb -clean-world",
    "re:build": "bsb -make-world",
    "re:cleanbuild": "bsb -make-world -clean-world",
    "re:watch": "bsb -make-world -w",
    "get-graphql-schema": "get-graphql-schema http://localhost:8000/___graphql -j > graphql_schema.json"
  },
  "dependencies": {
    "@moox/bs-react-helmet": "^3.0.0",
    "gatsby": "^2.24.11",
    "gatsby-image": "^2.4.13",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sharp": "^2.6.22",
    "gatsby-remark-images": "^3.3.21",
    "gatsby-remark-normalize-paths": "^1.0.0",
    "gatsby-remark-relative-images": "^0.3.0",
    "gatsby-source-filesystem": "^2.3.23",
    "gatsby-transformer-remark": "^2.8.27",
    "gatsby-transformer-sharp": "^2.5.11",
    "gatsby-transformer-yaml": "^2.4.10",
    "re-classnames": "^5.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "reason-nodejs": "^13.0.0-beta.1",
    "reason-promise": "^1.1.1",
    "reason-react": "^0.9.1"
  },
  "devDependencies": {
    "@reasonml-community/graphql-ppx": "^1.0.0-beta.21",
    "bs-platform": "^8.0.3",
    "get-graphql-schema": "^2.1.2"
  },
  "keywords": [
    "gatsby",
    "forestry"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kendallstrautman/brevifolia-gatsby-forestry"
  }
}
