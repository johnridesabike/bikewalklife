{
  "name": "bike-walk-life",
  "version": "0.1.0",
  "private": true,
  "description": "The source code for the Bike Walk Life blog.",
  "keywords": [
    "forestry"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnridesabike/bikewalklife.git"
  },
  "license": "GPL-3.0-or-later",
  "author": "John Jackson",
  "scripts": {
    "analyze": "npm-run-all build analyze:*",
    "analyze:js": "source-map-explorer _site/**/*.js _site/**/*.js.map",
    "analyze:html": "DEBUG=Eleventy:Benchmark* npm run build:html",
    "build": "npm-run-all -l build:rescript build:assets build:html",
    "build:assets": "NODE_ENV=production webpack",
    "build:html": "ELEVENTY_ENV=production eleventy --quiet",
    "build:rescript": "bsb -make-world",
    "clean": "npm-run-all -l --parallel clean:*",
    "clean:rescript": "bsb -clean-world -make-world",
    "clean:site": "rm -r _site",
    "postinstall": "bsb -make-world",
    "prestart": "NODE_ENV=development webpack",
    "serve": "serve _site",
    "start": "npm-run-all -l --parallel start:*",
    "start:assets": "NODE_ENV=development webpack --watch",
    "start:html": "ELEVENTY_ENV=development eleventy --serve --port=8080",
    "start:rescript": "bsb -w"
  },
  "dependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@11ty/eleventy-img": "^2.0.1",
    "@babel/core": "^7.19.1",
    "@fontsource/fira-sans": "^4.2.3",
    "@fontsource/source-serif-pro": "^4.2.0",
    "@reach/visually-hidden": "^0.13.0",
    "@rescript/react": "^0.10.1",
    "@ryyppy/rescript-promise": "^2.0.0",
    "acutis-lang": "^0.18.1",
    "browserslist": "^4.21.4",
    "bs-fetch": "^0.6.2",
    "bs-platform": "^9.0.2",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.1.0",
    "esbuild-loader": "^2.20.0",
    "feather-icons": "^4.28.0",
    "html-minifier": "^4.0.0",
    "js-yaml": "^4.1.0",
    "markdown-it": "^12.3.2",
    "mini-css-extract-plugin": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "postcss-loader": "^7.0.1",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.8.2",
    "re-formality": "^4.0.0-beta.17",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "source-map-explorer": "^2.5.2",
    "svgo": "^2.8.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-manifest-plugin": "^5.0.0"
  }
}
