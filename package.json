{
  "name": "bike-walk-life",
  "private": true,
  "description": "The source code for the Bike Walk Life blog.",
  "license": "MIT",
  "version": "0.1.0",
  "author": "John Jackson",
  "scripts": {
    "build": "bsb -make-world && gatsby build",
    "start": "concurrently \"bsb -w\" \"gatsby develop\"",
    "serve": "gatsby serve",
    "forestry-preview": "gatsby develop -p 8080 -H 0.0.0.0",
    "clean": "bsb -clean-world && gatsby clean",
    "re:clean": "bsb -clean-world",
    "re:build": "bsb -make-world",
    "re:cleanbuild": "bsb -make-world -clean-world",
    "re:watch": "bsb -w",
    "get-graphql-schema": "get-graphql-schema http://localhost:8000/___graphql -j > graphql_schema.json && jsonsort graphql_schema.json",
    "lint:css": "stylelint src/**/*.css",
    "lint:js": "eslint *.js src/**/*.js",
    "test": "yarn lint:css && yarn lint:js",
    "analyze": "ANALYZE_BUNDLE_SIZE=true yarn build",
    "postinstall": "bsb -make-world"
  },
  "dependencies": {
    "@reach/skip-nav": "^0.10.5",
    "@reach/visually-hidden": "^0.10.4",
    "bs-fetch": "^0.6.2",
    "charter-webfont": "^3.0.2",
    "gatsby": "^2.24.15",
    "gatsby-image": "^2.4.14",
    "gatsby-plugin-feed": "^2.5.11",
    "gatsby-plugin-manifest": "^2.4.21",
    "gatsby-plugin-nprogress": "^2.3.10",
    "gatsby-plugin-postcss": "^2.3.11",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-robots-txt": "^1.5.1",
    "gatsby-plugin-sharp": "^2.6.24",
    "gatsby-plugin-sitemap": "^2.4.11",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-remark-copy-linked-files": "^2.3.15",
    "gatsby-remark-images": "^3.3.23",
    "gatsby-remark-normalize-paths": "^1.0.0",
    "gatsby-source-filesystem": "^2.3.23",
    "gatsby-transformer-remark": "^2.8.27",
    "gatsby-transformer-sharp": "^2.5.12",
    "gatsby-transformer-yaml": "^2.4.10",
    "normalize.css": "^8.0.1",
    "postcss-custom-media": "^7.0.8",
    "postcss-custom-properties": "^9.1.1",
    "re-classnames": "^5.0.1",
    "re-formality": "^4.0.0-beta.8",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-feather": "^2.0.8",
    "react-helmet": "^6.1.0",
    "reason-promise": "^1.1.1",
    "reason-react": "^0.9.1",
    "typeface-cooper-hewitt": "0.0.44"
  },
  "devDependencies": {
    "@reasonml-community/graphql-ppx": "^1.0.0",
    "bs-platform": "^8.2.0",
    "concurrently": "^5.3.0",
    "gatsby-plugin-goatcounter": "^0.4.0",
    "get-graphql-schema": "^2.1.2",
    "json-sort-cli": "^1.15.25",
    "stylelint": "^13.6.1",
    "stylelint-config-wordpress": "^17.0.0"
  },
  "keywords": [
    "gatsby",
    "forestry"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnridesabike/bikewalklife.git"
  }
}
