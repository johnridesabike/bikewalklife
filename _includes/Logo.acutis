{% interface
  url_index = string
  current = string
  title_site = string
%}
<!-- Logo -->
{% Link
  class=!"header__title-link"
  href=url_index
  tabIndex=!-1
  current=!current
~%}
  <picture>
    <source
      srcset="/assets/images/2023/header-dark-2023.svg"
      media="(prefers-color-scheme: dark)">
    <img
      src="/assets/images/2023/header-light-2023.svg"
      alt="{{ title_site }}"
      height=80
      width=256 />
  </picture>
{%~ /Link %}
<!-- /Logo -->
