{% interface
  href = string
  strings = {open_linked: string}
/interface %}
<!-- Entry_OriginalLink -->
<a href="{{ href }}" target="_blank" rel="noopener" class="u-in-reply-to">
  {%~ match strings with {open_linked} %} {{ open_linked }} {% /match ~%}
  <span aria-hidden="true">
    {%~ Icon name="external-link" class=!"icon" / ~%}
  </span>
</a>
<!-- /Entry_OriginalLink -->
